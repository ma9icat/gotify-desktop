{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "http://localhost:1420",
    "distDir": "../src"
  },
  "tauri": {
    "identifier": "com.gotify.desktop",
    "productName": "gotify-desktop",
    "version": "0.2.0",
    "allowlist": {
      "all": false,
      "shell": {
        "open": true
      },
      "dialog": {
        "open": true,
        "save": true
      },
      "fs": {
        "readFile": true,
        "writeFile": true
      },
      "window": {
        "all": true,
        "close": true,
        "startDragging": true,
        "setAlwaysOnTop": true,
        "setTitle": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.gotify.desktop",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "windows": [
      {
        "title": "Gotify Desktop",
        "width": 900,
        "height": 700,
        "minWidth": 600,
        "minHeight": 400,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}