{
  "$schema": "https://schema.tauri.app/config/2.7.0",
  "productName": "gotify-desktop",
  "version": "0.2.0",
  "identifier": "com.gotify.desktop",
  "build": {
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "features": []
  },
  "app": {
    "windows": [
      {
        "title": "Gotify Desktop",
        "width": 900,
        "height": 700,
        "minWidth": 600,
        "minHeight": 400,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "identifier": "com.gotify.desktop"
  }
}